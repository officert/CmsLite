@model CmsLite.Core.Areas.Admin.Models.AdminLayoutModel
<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9]>    <html class="no-js ie9" lang="en"> <![endif]-->
<!--[if gt IE 9]><!-->
<html class="no-js" lang="en">
<!--<![endif]-->
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cms Lite | @ViewBag.Title</title>
    <!-- css -->
    <link rel="stylesheet" href="@Url.Content("~/Areas/Admin/Content/cms.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Areas/Admin/Content/fontawesome/font-awesome.min.css")" />
    <!-- scripts -->
</head>
<body>
    @Html.Partial("_Header", Model.UserMenu)
    <div id="wrapper">
        <div id="sidebar-wrapper" class="col-md-1">
            <div id="sidebar">
                @Html.Partial("_Sidebar", Model.SidebarMenu)
            </div>
        </div>
        <div id="main-wrapper" class="col-md-11">
            <div id="main">
                @RenderBody()
            </div>
        </div>
    </div>
    <div class="modal hide" id="loading">
        <img alt="loading" src="@Url.Content("~/Areas/Admin/Content/images/loading.gif")"/>
    </div>
    <script src="@Url.Content("~/Areas/Admin/scripts/libs/jquery-1.10.2.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/libs/jquery-ui-1.10.3.custom.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/libs/jquery.validate.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/unobtrusiveValidationOverrides.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/libs/jquery.validate.unobtrusive.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/libs/knockout-2.2.1.debug.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/libs/bootstrap.js")"></script>
    
    <script src="@Url.Content("~/Areas/Admin/scripts/source/core.cms.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/core.utils.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/core.mapping.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/ko.bindinghandlers/ko.bootstrappopover.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/ko.bindinghandlers/ko.bootstrapmodal.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/ko.bindinghandlers/ko.slidevisible.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/plugins/jquery.accordion.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/plugins/jquery.contextmenu.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/scripts/source/plugins/jquery.simpleTabs.js")"></script>
    @*@Scripts.Render("~/bundles/Libs", "~/bundles/Source")*@
    <script>
        (function (cms) {
            cms.utils.mapPath = function (path) {
                return path.replace('~/', '@Url.Content("~")');
            };
        })(cms);
    </script>
    @RenderSection("Scripts", false)
</body>
</html>
